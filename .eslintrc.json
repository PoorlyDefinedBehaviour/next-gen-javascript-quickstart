{
  "parser": "babel-eslint",
  "env": {
    "browser": false,
    "es6": true
  },
  "extends": ["airbnb-base", "plugin:prettier/recommended"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 2019,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": [2, { "args": "all", "argsIgnorePattern": "^_" }]
  }
}
